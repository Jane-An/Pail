<%- include('header') -%>
<div class="jumbotron">
   <h1>Profile for <%= profile.userName %></h1>
   <img src="<%= profile.profilePicURL%>" width="200">
   <hr>
   <p> Welcome <%= profile.googlename %>
       with email <%= profile.googleemail %>
       You are logged in.
   </p>

   Nickname: <%= profile.userName %>
   Age: <%= profile.age %>
   Gender:<%= profile.gender %>

   <p> Last updated on <%= profile.lastUpdate %> </p>
</div>


<%- include('footer') -%>
